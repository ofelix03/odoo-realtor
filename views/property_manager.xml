<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>
        <record id="property_manager_form" model="ir.ui.view">
            <field name="name">realtor.property.manager.form</field>
            <field name="model">realtor.property.manager</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <group>
                                <field name="name" />
                                <field name="mobile" />
                                <field name="email" />
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="property_manager_tree" model="ir.ui.view">
            <field name="name">realtor.property.manager.tree</field>
            <field name="model">realtor.property.manager</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name" />
                    <field name="mobile" />
                    <field name="email" />
                </tree>
            </field>
        </record>

        <record id="property_manager_action" model="ir.actions.act_window">
            <field name="name">Property Managers</field>
            <field name="res_model">realtor.property.manager</field>
            <field name="view_mode">tree,form</field>
        </record>

    </data>
</odoo>
